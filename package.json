{
  "name": "cms-svelte-boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "rm -rf dist && webpack --mode production",
    "deploy": "rm -rf dist && webpack --mode production && hs upload dist cms-svelte-boilerplate",
    "prettier:write": "prettier --write 'src/**/*.js' 'src/**/*.json'",
    "start": "rm -rf dist && webpack --watch --env.autoupload --mode production",
    "upload": "hs upload dist cms-svelte-boilerplate"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "core-js": "^3.6.5"
  },
  "devDependencies": {
    "@hubspot/cms-cli": "^1.1.9",
    "@hubspot/webpack-cms-plugins": "^1.1.7",
    "autoprefixer": "^9.8.5",
    "babel-eslint": "^10.1.0",
    "copy-webpack-plugin": "^6.0.3",
    "css-loader": "^5.0.1",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.3",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.12.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.19.1",
    "regenerator-runtime": "^0.13.7",
    "sass-loader": "^8.0.2",
    "svelte": "^3.31.0",
    "svelte-loader": "^2.13.6",
    "url-loader": "^4.1.1",
    "webpack": "^4.44.0",
    "webpack-cli": "^3.3.12"
  }
}
